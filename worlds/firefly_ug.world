    </include>
    <include>
      <name>cone_3</name>
      <uri>model://construction_cone</uri>
      <pose>2 0 0 0 0 0</pose>
    </include>
    <include>
      <name>cone_4</name>
      <uri>model://construction_cone</uri>
      <pose>2 2 0 0 0 0</pose>
    </include>
    <include>
      <uri>model://depo_area</uri>
      <pose>0.2 -1.34 0 0 0 0</pose>
    </include>
    <include>
